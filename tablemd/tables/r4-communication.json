{
    "tables": [
        {
            "terminology": {
                "fields": [
                    {
                        "description": "The type of the FHIR resource.",
                        "example": "{\n  \"resourceType\": \"Communication\"\n}\n",
                        "name": "resourceType",
                        "note": "The resourceType must be 'Communication'.",
                        "required": "Yes",
                        "type": "<a target=\"_blank\" href=\"https://hl7.org/fhir/R4/datatypes.html#string\"><code>string</code></a>",
                        "url": "https://hl7.org/fhir/r4/Communication-definitions.html#Communication"
                    },
                    {
                        "description": "The status of the communication.",
                        "example": "{\n  \"status\": \"completed\"\n}\n",
                        "name": "status",
                        "note": "All communications must be in 'completed' status.",
                        "required": "Yes",
                        "type": "<a target=\"_blank\" href=\"https://hl7.org/fhir/R4/datatypes.html#code\"><code>code</code></a>",
                        "url": "https://hl7.org/fhir/r4/communication-definitions.html#Communication.status"
                    },
                    {
                        "binding": {
                            "description": "The category of the communication.",
                            "terminology": [
                                {
                                    "display": "Communication Category",
                                    "info_link": "https://hl7.org/fhir/R4/valueset-communication-category.html",
                                    "system": "http://terminology.hl7.org/CodeSystem/communication-category"
                                },
                                {
                                    "display": "SNOMED CT",
                                    "info_link": "https://hl7.org/fhir/r4/snomedct.html",
                                    "system": "http://snomed.info/sct"
                                }
                            ]
                        },
                        "description": "Codeable definition of the communication type.",
                        "example": "{\n  \"category\": [\n    {\n      \"coding\": [\n        {\n          \"system\": \"http://terminology.hl7.org/CodeSystem/communication-category\",\n          \"code\": \"notification\"\n        }\n      ]\n    }\n  ]\n}\n",
                        "name": "category",
                        "note": "<ul>\n  <li>Only CommunicationCategory values of <code>notification</code> and <code>reminder</code> and SNOMED CT <code>312853008</code> are supported.</li>\n</ul>\n",
                        "required": "Yes",
                        "type": "<a target=\"_blank\" href=\"https://hl7.org/fhir/R4/datatypes.html#CodeableConcept\"><code>CodeableConcept</code></a>",
                        "url": "https://hl7.org/fhir/r4/communication-definitions.html#Communication.category"
                    },
                    {
                        "description": "The priority of the communication.",
                        "example": "{\n  \"priority\": \"routine\"\n}\n",
                        "name": "priority",
                        "note": "Only 'routine' and 'urgent' priorities are supported.",
                        "required": "No",
                        "type": "<a target=\"_blank\" href=\"https://hl7.org/fhir/R4/datatypes.html#code\"><code>code</code></a>",
                        "url": "https://hl7.org/fhir/r4/communication-definitions.html#Communication.priority"
                    },
                    {
                        "description": "The patient that is the focus of the communication.",
                        "example": "{\n  \"subject\": {\n    \"reference\": \"Patient/9836548\"\n  }\n}\n",
                        "name": "subject",
                        "note": "Only Patient is supported as subject.",
                        "required": "No",
                        "type": "<a target=\"_blank\" href=\"https://hl7.org/fhir/R4/references.html\"><code>Reference</code></a> (<a target=\"_blank\" href=\"https://hl7.org/fhir/R4/patient.html\"><code>Patient</code></a>)",
                        "url": "https://hl7.org/fhir/r4/communication-definitions.html#Communication.subject"
                    },
                    {
                        "description": "The topic of the communication, similar to the subject field of an email.",
                        "example": "{\n  \"topic\" : {\n     \"text\": \"Patient Consult Request\"\n  }\n}\n",
                        "name": "topic",
                        "note": "Currently only free-text topics are supported.",
                        "required": "No",
                        "type": "<a target=\"_blank\" href=\"https://hl7.org/fhir/R4/datatypes.html#code\"><code>code</code></a>",
                        "url": "https://hl7.org/fhir/r4/communication-definitions.html#Communication.topic"
                    },
                    {
                        "description": "The encounter associated to the communication.",
                        "example": "{\n  \"encounter\" : {\n    \"reference\" : \"Encounter/89363836\"\n  }\n}\n",
                        "name": "encounter",
                        "note": "Subject is required when writing an encounter.",
                        "required": "No",
                        "type": "<a target=\"_blank\" href=\"https://hl7.org/fhir/R4/references.html\"><code>Reference</code></a> (<a target=\"_blank\" href=\"https://hl7.org/fhir/R4/encounter.html\"><code>Encounter</code></a>)",
                        "url": "https://hl7.org/fhir/r4/communication-definitions.html#Communication.encounter"
                    },
                    {
                        "description": "The entity receiving the communication.",
                        "example": "{\n  \"recipient\": [\n    {\n      \"reference\" : \"Practitioner/6373634\"\n    },\n    {\n      \"reference\" : \"Group/44466678\"\n    }\n  ]\n}\n",
                        "name": "recipient",
                        "note": "Each recipient must be a Practitioner or Group.",
                        "required": "Yes",
                        "type": "<a target=\"_blank\" href=\"https://hl7.org/fhir/R4/references.html\"><code>Reference</code></a> (<a target=\"_blank\" href=\"https://hl7.org/fhir/R4/practitioner.html\"><code>Practitioner</code></a>, <a target=\"_blank\" href=\"https://hl7.org/fhir/R4/group.html\"><code>Group</code></a>)",
                        "url": "https://hl7.org/fhir/r4/communication-definitions.html#Communication.recipient"
                    },
                    {
                        "description": "The entity sending the communication.",
                        "example": "{\n  \"sender\": {\n    \"reference\": \"Practitioner/631923\"\n  }\n}\n",
                        "name": "sender",
                        "note": "The sender must be a Practitioner.",
                        "required": "Yes",
                        "type": "<a target=\"_blank\" href=\"https://hl7.org/fhir/R4/references.html\"><code>Reference</code></a> (<a target=\"_blank\" href=\"https://hl7.org/fhir/R4/practitioner.html\"><code>Practitioner</code></a>)",
                        "url": "https://hl7.org/fhir/r4/communication-definitions.html#Communication.sender"
                    },
                    {
                        "description": "The text of the communication.",
                        "example": "{\n  \"payload\" : [\n    {\n      \"contentAttachment\" : {\n        \"contentType\" : \"application/xhtml+xml\",\n        \"data\" : \"PCFET0NUWVBFIGh0bWwgUFVCTElDICItLy9XM0MvL0RURCBYSFRNTCAxLjAgVHJhbnNpdGlvbmFsLy9FTiIKImh0dHA6Ly93d3cudzMub3JnL1RSL3hodG1sMS9EVEQveGh0bWwxLXRyYW5zaXRpb25hbC5kdGQiPjxodG1sPjxib2R5PjwvYm9keT48L2h0bWw+\"\n      }\n    }\n  ]\n}\n",
                        "name": "payload",
                        "note": "<ul>\n  <li>content must be supplied as a contentAttachment with contentType and data defined</li>\n  <li>contentType can be set to:\n    <ul>\n      <li>application/xhtml+xml</li>\n      <li>text/plain</li>\n    </ul>\n  </li>\n  <li>contentType can contain a charset set to:\n    <ul>\n      <li>utf-8</li>\n      <li>us-ascii</li>\n    </ul>\n  </li>\n  <li>data should be base64 encoded</li>\n  <li>If the contentType is \"application/xhtml+xml\", then the data (when decoded) should at least contain the\n      following:\n      <pre><!DOCTYPE html PUBLIC \"-//W3C//DTD XHTML 1.0 Transitional//EN\"\n      \"http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd\"><html><body></body></html></pre>\n  </li>\n</ul>\n",
                        "required": "Yes",
                        "type": "<a target=\"_blank\" href=\"https://hl7.org/fhir/R4/backboneelement.html\"><code>BackboneElement</code></a>",
                        "url": "https://hl7.org/fhir/r4/communication-definitions.html#Communication.payload"
                    },
                    {
                        "binding": {
                            "description": "The status of the email. May be received, in-progress or completed.",
                            "terminology": [
                                {
                                    "display": "Communication Email Status Extension",
                                    "info_link": "https://www.hl7.org/fhir/r4/valueset-task-status.html",
                                    "system": "http://hl7.org/fhir/task-status"
                                }
                            ]
                        },
                        "description": "The status of the email. May be received, in-progress or completed.",
                        "name": "email-status",
                        "url": "https://fhir-ehr.cerner.com/r4/StructureDefinition/email-status?_format=json"
                    },
                    {
                        "description": "The entity replies will be directed to.",
                        "example": "{\n  \"url\": \"https://fhir-ehr.cerner.com/r4/StructureDefinition/reply-to\",\n  \"valueReference\": {\n    \"reference\": \"Group/123\"\n  }\n}\n",
                        "name": "extension[x].valueReference",
                        "note": "The reply-to must be a Group.",
                        "required": "No",
                        "type": "<a target=\"_blank\" href=\"https://hl7.org/fhir/R4/references.html\"><code>Reference</code></a> (<a target=\"_blank\" href=\"https://hl7.org/fhir/R4/group.html\"><code>Group</code></a>)",
                        "url": "https://fhir-ehr.cerner.com/r4/StructureDefinition/reply-to?_format=json"
                    }
                ],
                "name": "Communication",
                "table_name": "terminology"
            }
        },
